{
    "name": "waygou/multi-tenant",
    "description": "Apply multi-tenancy to Xheetah.",
    "type": "library",
    "keywords": [
        "laravel",
        "php",
        "hyn",
        "waygou",
        "multi tenant"
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.7.*",
        "waygou/helpers": "dev-master",
        "hyn/multi-tenant": "^5.3",
        "plesk/api-php-lib": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Waygou\\MultiTenant\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waygou\\MultiTenant\\ServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Waygou",
            "email": "bruno.falcao@waygou.com"
        }
    ],
    "minimum-stability": "dev"
}
